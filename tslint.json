{
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-config-prettier",
    "tslint-config-airbnb"
  ],
  "rules": {
    "function-name": false,
    "import-name": false,
    "no-boolean-literal-compare": false,
    "object-literal-sort-keys": [
      true,
      "shorthand-first"
    ],
    "variable-name": false
  },
  "linterOptions": {
    "exclude": [
      "src/types"
    ]
  }
}
